{
  "title": "Excel",
  "rules": [
    {
      "description": "excel nav",
      "manipulators": [{

        "type": "basic",
        "from": { "key_code": "p", "modifiers": { "mandatory": [ "control" ] } },
        "to": [{ "key_code": "left_arrow", "modifiers": "option" }],
        "conditions": [{
          "type": "frontmost_application_if",
          "bundle_identifiers": [
            "^com\\.microsoft\\.Excel"
          ]
        }]
      },
      {
        "type": "basic",
        "from": { "key_code": "n", "modifiers": { "mandatory": [ "control" ] } },
        "to": [{ "key_code": "right_arrow", "modifiers": "option" }],
        "conditions": [{
          "type": "frontmost_application_if",
          "bundle_identifiers": [
            "^com\\.microsoft\\.Excel"
          ]
        }
        ]
      },
      {
        "type": "basic",
        "from": { "key_code": "w", "modifiers": { "mandatory": [ "control" ] } },
        "to": [{ "key_code": "escape" }],
        "conditions": [{
          "type": "frontmost_application_if",
          "bundle_identifiers": [
            "^com\\.microsoft\\.Excel"
          ]
        }
        ]
      }
      ]
    }
  ]
}
